{
  "name": "watermark-div",
  "version": "0.1.5",
  "description": "a beautiful watermak to your html element",
  "main": "dist/Watermark.js",
  "scripts": {
    "dev": "babel src -d dist --watch",
    "build": "babel src -d dist"
  },
  "author": "liugq",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.14.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": "umd"
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}